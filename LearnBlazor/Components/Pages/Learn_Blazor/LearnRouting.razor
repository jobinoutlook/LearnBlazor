@page "/learnRouting"
@page "/learnRouting/{Param1:int}"
@page "/learnRouting/{Param1:int}/{Param2}"
@page "/learnBlazor/learnRouting"

<h3>LearnRouting</h3>

<p>Param1 : @Param1 </p>
@if (Param2 != null)
{
    <p>Param2 : @Param2 </p>
}
@code {
    [Parameter]
    public int Param1 { get; set; }
    [Parameter]
    public string Param2 { get; set; }
}
